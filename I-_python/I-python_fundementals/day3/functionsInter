import random

def randInt(min=0, max=100):
    
    if min > max:
        min, max = max, min
    
    if min == max:
        return min
    
    num = random.random() * (max - min) + min
    return round(num)

print(randInt(min=80, max=30))   
print(randInt(min=42, max=42))   
print(randInt(min=-50, max=-10)) 
